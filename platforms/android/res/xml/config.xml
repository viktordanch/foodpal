<?xml version='1.0' encoding='utf-8'?>
<widget id="com.foodpal.foodpal" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Foodpal.com</name>
    <description>
        Foodpal.com website client app.
    </description>
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <access origin="*" />
    <access origin="http://google.com" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="exit-on-suspend" value="true" />
    <feature name="App">
        <param name="android-package" value="com.foodpal.foodpal.App" />
    </feature>
    <author email="info@foodpal.com" href="http://foodpal.com">
        Apache Cordova Team
    </author>
    <plugin name="SplashScreen" value="org.apache.cordova.SplashScreen"/>
    <plugin name="Geolocation" value="org.apache.cordova.GeoBroker" />
    <content src="index.html" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <feature name="Device">
        <param name="android-package" value="com.foodpal.foodpal.device.Device" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.InAppBrowser" />
    </feature>
</widget>
